<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  <param-name>cgst</param-name>
  <param-value>0.20</param-value>
  </context-param>
 
  <servlet>
  <init-param>
  <param-name>sgstofkar</param-name>
  <param-value>0.10</param-value>
  </init-param>
  <servlet-name>karnataka</servlet-name>
  <servlet-class>gst.KarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>karnataka</servlet-name>
  <url-pattern>/kar</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <init-param>
  <param-name>sgstoftn</param-name>
  <param-value>0.12</param-value>
  </init-param>
  <servlet-name>tamil</servlet-name>
  <servlet-class>gst.TnServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>tamil</servlet-name>
  <url-pattern>/tn</url-pattern>
  </servlet-mapping>
  
  
</web-app>
